{
  "examples": {
    "search_flights": {
      "description": "Search for flights from JFK to LHR on March 15, 2024",
      "request": {
        "method": "tools/call",
        "params": {
          "name": "search_flights",
          "arguments": {
            "from": "JFK",
            "to": "LHR",
            "date": "2024-03-15",
            "include_connecting": true
          }
        }
      }
    },
    "search_flights_range": {
      "description": "Search for flights from LAX to NRT between March 15-20, 2024, departing between 6 AM and 6 PM",
      "request": {
        "method": "tools/call",
        "params": {
          "name": "search_flights_range",
          "arguments": {
            "from": "LAX",
            "to": "NRT",
            "start_date": "2024-03-15",
            "end_date": "2024-03-20",
            "start_time": "06:00",
            "end_time": "18:00",
            "include_connecting": true
          }
        }
      }
    },
